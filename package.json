{
  "name": "workshop-backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register main.ts",
    "start": "ts-node -r tsconfig-paths/register main",
    "prisma:merge": "cat prisma/schema/**.prisma > prisma/schema.prisma",
    "prisma:format": "yarn prisma format --schema prisma/schema.prisma",
    "prisma:migrate": "yarn prisma migrate dev --name init --schema prisma/schema.prisma",
    "prisma:generate": "yarn pnpify prisma generate --schema=prisma/schema.prisma",
    "test": "jest --verbose",
    "test:bld": "jest -- src/app.spec.ts",
    "test:seed": "jest -- src/seedData.spec.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@supabase/supabase-js": "^2.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@yarnpkg/pnpify": "^4.0.0-rc.40",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-extra": "^0.11.6",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/uuid": "^9.0.1",
    "jest": "^29.5.0",
    "prisma": "^4.11.0",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
